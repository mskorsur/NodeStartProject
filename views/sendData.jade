doctype html
html
  head
    meta(charset="utf-8")
    title Example program
  body
    h2 #{title}
    h4 #{message}
    ul
    each student in students
      li
        ul
          li Student's number - #{student.studentNumber}
          li  Full name - #{student.name}
          li Program - #{student.program}
          li Subjects - #{student.courses} 
      
   